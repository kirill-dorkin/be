{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": true
    },
    "transform": {
      "react": {
        "runtime": "automatic"
      }
    },
    "target": "es2022"
  },
  "module": {
    "type": "commonjs"
  },
  "sourceMaps": true,
  "env": {
    "test": {
      "jsc": {
        "transform": {
          "react": {
            "runtime": "automatic"
          }
        }
      }
    }
  }
}
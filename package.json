{
  "name": "be.kg",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:standard": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --selectProjects unit",
    "test:integration": "jest --config jest.integration.config.js",
    "test:all": "jest --selectProjects unit integration",
    "e2e": "playwright test",
    "perf:audit": "node scripts/performance-audit.mjs",
    "perf:audit:ci": "npm run build && npm run start & sleep 5 && npm run perf:audit http://localhost:3000 && kill %1",
    "perf:analyze": "node scripts/performance-analysis.mjs",
    "perf:full": "npm run perf:analyze && npm run lighthouse:ci",
    "bundle:analyze": "ANALYZE=true npm run build",
    "bundle:check": "node scripts/analyze-bundle.js",
    "bundle:report": "npm run build && npm run bundle:check",
    "lighthouse:ci": "lhci autorun",
    "lighthouse:local": "lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html --chrome-flags=\"--headless\"",
    "preview": "npm run build && npm run start"
  },
